page {
    width: 750rpx;
    background: #f5f5f5;
}

.search {
    width: 750rpx;
    height: 104rpx;
    background: #fff;
    display: flex;
    align-items: center;
    border-top: 1px solid #f5f5f5;
}

.search .search_area {
    width: 610rpx;
    height: 72rpx;
    background: rgba(0, 0, 0, 0.08);
    border-radius: 20rpx;
    margin-left: 32rpx;
    display: flex;
    align-items: center;
}

.search .search_area .search_icon {
    width: 28rpx;
    height: 28rpx;
    margin-left: 24rpx;
}

.search .search_area .search_input {
    width: 520rpx;
    height: 72rpx;
    font-size: 30rpx;
    padding-left: 20rpx;
}

.search .do_search {
    font-size: 30rpx;
    font-family: PingFangSC-Regular;
    color: rgba(27, 103, 255, 1);
    padding-left: 20rpx;
}
